<div class="studentsidebar">
    <h2>Học sinh</h2>
    <div class="student-list">
        {{#each student}}
        <div class="student-item">
            <span class="student-name">{{name}}</span>
            <span class="student-score">{{score}}</span>
        </div>
        <hr>
        {{/each}}
    </div>
    <button class="complete-button">Hoàn Thành</button>
</div>
